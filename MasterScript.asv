C_D = 1.4*ones(1,1000);
C_L = zeros(1,1000);
% need to fix this parameter:
% time = ???
C_D_change = {};
C_D_change{1} = C_D;
[M1, V, Z, time] = numerical_soln_take2(C_D, C_L, time);
[C_D, C_L] = pressure_calc(M1, V, Z);
C_D_change{2} = C_D;

i = 2;
while C_D_change{i} - C_D_change{i-1} > 1
    [M1, V, Z, time] = numerical_soln_take2(C_D, C_L, time);
    [C_D, C_L] = pressure_calc(M1, V, Z);
    C_D_change{i+1} = C_D;
    i = i + 1;
end

